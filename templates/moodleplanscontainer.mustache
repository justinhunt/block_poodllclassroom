<div class="block_poodllclassroom_togglediv">
    <div class="btn-group" role="group" aria-label="Basic example">
        <div class="btn-group btn-group-toggle" data-toggle="buttons">
            <label class="btn btn-primary active togglelink toggle-left-button">
                <input type="radio" name="options" id="option1" autocomplete="off" checked> 350 Users
            </label>
            <label class="btn btn-secondary togglelink toggle-right-button">
                <input type="radio" name="options" id="option3" autocomplete="off"> 600 Users
            </label>
            </div>
        </div>
    </div>

<div class="main-pack desktop">
    <table class="table poodllclassroom_pricing">
    <thead>
    <tr>
        <th scope="col" class="title_features">Features</th>
        <th scope="col">
            <div class="poodllclassroom_pricing_column_heading">
                <h4>{{#str}}mediaplans, block_poodllclassroom{{/str}}</h4>
                {{#mediaplans}}
                    <h6 class="block_poodllclassroom_prices {{display}}"><span class="block_poodllclassroom_prices_currency">US$</span>{{price}}<span class="block_poodllclassroom_prices_period">/Year</span></h6>
                {{/mediaplans}}
            </div>
        </th>
        <th scope="col">
            <div class="poodllclassroom_pricing_column_heading">
                <h4>{{#str}}langplans, block_poodllclassroom{{/str}}</h4>
