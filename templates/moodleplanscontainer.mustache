<div class="block_poodllclassroom_billingheader {{billinginterval}} {{display}}"><h3>{{billingintervallabel}}</h3></div>
<h4>{{#str}}mediaplans, block_poodllclassroom{{/str}}</h4>
<div id="block_poodllclassroom_mediaplans_cont" class="block_poodllclassroom_moodleplans_cont center block_poodllclassroom_flexbox {{billinginterval}} {{display}}">
    {{#mediaplans}}
        {{#checkoutoutexisting}}
            {{> block_poodllclassroom/upgradetile}}
        {{/checkoutoutexisting}}
        {{^checkoutoutexisting}}
            {{> block_poodllclassroom/newplantile}}
        {{/checkoutoutexisting}}
    {{/mediaplans}}
</div>
