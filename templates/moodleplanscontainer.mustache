        <td class="feature_available"></td>
        <td class="feature_available"></td>
        <td class="feature_unavailable"></td>
        </tr>
        <tr>
        <th scope="row"></th>
        <td>
            <div class="poodllclassroom_pricing_column_buttons media">
            {{#mediaplans}}
                <div class="block_poodllclassroom_cbbuttons {{display}}">
                    {{#checkoutexisting}}
                        {{> block_poodllclassroom/upgradetile}}
                    {{/checkoutexisting}}
                    {{^checkoutexisting}}
                        {{> block_poodllclassroom/newplantile}}
                    {{/checkoutexisting}}
                </div>
            {{/mediaplans}}
            </div>
        </td>
        <td>
            <div class="poodllclassroom_pricing_column_buttons lang">
            {{#langplans}}
                 <div class="block_poodllclassroom_cbbuttons {{display}}">
                    {{#checkoutexisting}}
