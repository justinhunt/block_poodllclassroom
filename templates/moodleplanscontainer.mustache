        <td class="feature_available"></td>
        <td class="feature_unavailable"></td>
        <td class="feature_available"></td>
        </tr>
        <tr>
        <th scope="row">{{#str}}featureSix, block_poodllclassroom{{/str}}</th>
        <td class="feature_available"></td>
        <td class="feature_available"></td>
        <td class="feature_unavailable"></td>
        </tr>
        <tr>
        <th scope="row">{{#str}}featureSeven, block_poodllclassroom{{/str}}</th>
