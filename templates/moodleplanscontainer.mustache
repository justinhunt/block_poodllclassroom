                {{/essentialsplans}}
            </div>
        </th>
    </tr>
    </thead>
    <tbody>
        <tr>
        <th scope="row">{{#str}}featureOne, block_poodllclassroom{{/str}}</th>
        <td class="feature_available"></td>
        <td class="feature_unavailable"></td>
        <td class="feature_available"></td>
        </tr>
        <tr>
        <th scope="row">{{#str}}featureTwo, block_poodllclassroom{{/str}}</th>
        <td class="feature_available"></td>
        <td class="feature_unavailable"></td>
        <td class="feature_available"></td>
        </tr>
        <tr>
        <th scope="row">{{#str}}featureThree, block_poodllclassroom{{/str}}</th>
        <td class="feature_available"></td>
