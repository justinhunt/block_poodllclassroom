<tr id="block_poodllclassroom_subs_row_{{id}}" class="block_poodllclassroom_subs_row" data-id="{{id}}">
    <td class="itemid cell c0" style="">{{plan.name}}</td>
    <td class="itemname cell c1" style="">{{plan.period_display}}</td>
    {{#paymentcurr}} <td class="itemcurrency cell c3" style="">{{paymentcurr}}</td>{{/paymentcurr}}
    {{#payment_display}} <td class="itempayment cell c4" style="">{{payment_display}}</td>{{/payment_display}}
    {{#status_display}} <td class="itemstatus cell c5" style="">{{{status_display}}}</td>{{/status_display}}
    <td class="itemdate cell c6" style="">{{timecreated_display}}</td>
    {{#expiretime_display}} <td class="itemdate cell c2" style="">{{expiretime_display}}</td>{{/expiretime_display}}
    <td class="itemedit cell c7" style="">
        <a class="block_poodllclassroom_subseditlink btn btn-secondary" data-cbaction='none' data-id="{{id}}" data-type="change" href="{{{changeurl}}}">{{#str}}changeplan, block_poodllclassroom{{/str}}</a>
        <a class="block_poodllclassroom_subsmanagelink btn btn-secondary" data-cbaction='ssp' data-id="{{id}}" data-upstreamsubid="{{upstreamsubid}}" data-upstreamownerid="{{school.upstreamownerid}}" data-type="subdetails" href="#">{{#str}}managesub, block_poodllclassroom{{/str}}</a>
        {{#editsuburl}}
            {{{editsuburl}}}
        {{/editsuburl}}
    </td>
    <td class="renew cell c8 lastcol" style="">
    {{#oldnotfreecondition_free}}
        <a class="block_poodllclassroom_renewlink block_poodllclassroom_gocbcheckout" data-method='get_checkout_existing' data-planid="{{plan.id}}" data-schoolid="{{school.id}}"
           data-currency="{{paymentcurr}}" data-currentsubid="{{id}}"  data-billinginterval="{{plan.billinginterval}}"  href="#">{{#str}}renewnow, block_poodllclassroom{{/str}}</a>
    {{/oldnotfreecondition_free}}
    </td>
</tr>